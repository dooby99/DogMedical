<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    tools:context=".image.ResultActivity">

    <com.example.petmedical.image.OverlayView
        android:id="@+id/overlay_view"
        android:layout_width="350dp"
        android:layout_height="550dp"
        android:layout_below="@id/image_view_result"
        app:layout_constraintBottom_toBottomOf="@+id/image_view_result"
        app:layout_constraintEnd_toEndOf="@+id/image_view_result"
        app:layout_constraintHorizontal_bias="0.478"
        app:layout_constraintStart_toStartOf="@+id/image_view_result"
        app:layout_constraintTop_toTopOf="@+id/image_view_result"
        app:layout_constraintVertical_bias="0.0" />

    <ImageView
        android:id="@+id/image_view_result"
        android:layout_width="350dp"
        android:layout_height="550dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.494"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.182"
        app:srcCompat="@drawable/ic_launcher_foreground" />

</androidx.constraintlayout.widget.ConstraintLayout>